
<div id="wods" class="transitions-enabled">
  <% @wods.each do |wod| %>
  <% if wod.user == current_user %>
    <div class="box panel panel-info">
    <div class="panel-heading"><b>WOD:</b> <%= wod.description %></div>
  <div class="panel-body">
    <% else %>
    <div class="box panel panel-danger">
    <div class="panel-heading"><b>Wod Name:</b> <%= wod.description %></div>
  <div class="panel-body">
    <% end %>
    
        <div class="list-group">
          <a href="#" class="list-group-item list-group-item-success"><b>Wod Name:</b> <%= wod.description %></a>
          <a href="#" class="list-group-item list-group-item-info"><%= wod.description %></a>
          <a href="#" class="list-group-item list-group-item-warning"><b>Time:</b> <%= wod.wodtime %></a>
          <a href="#" class="list-group-item list-group-item-danger">Vestibulum at eros</a>
        </div>
      
      
      <%= link_to 'Show', wod_path(wod) %>
      <% if wod.user == current_user %>
        <%= link_to 'Edit', edit_wod_path(wod) %>
        <%= link_to 'Destroy', wod, method: :delete, data: { confirm: 'Are you sure?' } %>
      <% end %>
  </div>
    </div>
  <% end %>
</div>

